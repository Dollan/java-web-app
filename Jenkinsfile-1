pipeline {
  agent any
  stages {
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sq1') {
          sh  'mvn sonar:sonar \n sonar.dependencyCheck.jsonReportPath=${WORKSPACE}/dependency-check-report.json
                \n sonar.dependencyCheck.htmlReportPath=${WORKSPACE}/dependency-check-report.html'
        }
      }
    }
  }
}
